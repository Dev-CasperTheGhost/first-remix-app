datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

generator client {
    provider      = "prisma-client-js"
    binaryTargets = ["native"]
}

model Week {
    id       String   @id @default(cuid())
    start    DateTime
    earnings Float
    hours    Float
    days     Int      @default(1)
}
